#include <stdio>
/**
 * main - Entry point
 * Return: Always 0 (Success)
 */
int main(void)
{
	printf("Size of a char: %c byte(s)\n", sizeof(c));
	printf("Size of an int: %d bytr(s)\n", sizeof(d));
	printf("Size of a long int: %ld byte(s)\n", sizeof(ld));
	printf("Size of a long long int: %lld bytrs(s)\n", sizeof(lld));
	printf("Size of a float: %f byte(s)\n", sizeof(f));
	return(0);
}
